#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char s[100];

    scanf("%[^\n]",s);
    int n=strlen(s);
    int i;
    for(i=0;i<n;i++)
    {
       if(s[i]==' ')
       {
           printf("\n");
       }
       else 
       {
           printf("%c",s[i]);
       }

    }

    return 0;
